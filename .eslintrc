{
  "plugins": [
    "react",
    "zmclient"
  ],
  "settings": {
    "react": {
      "version": "15.3.1"
    }
  },
  "env": {
    "browser": true,
    "es6": false,
    "jquery": true,
    "node": false
  },
  "extends": [ "eslint:recommended", "plugin:react/recommended" ],
  "parser": "espree",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script",
    "ecmaFeatures": {
      "jsx": true,
      "impliedStrict": false,
      "modules": false
    }
  },
  "rules": {
    "comma-dangle": [
      2,
      "never"
    ],
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": [2, {
      "allowEmptyCatch": true
    }],
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 0,
    "use-isnan": 2,
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,
    "accessor-pairs": 2,
    "array-callback-return": 2,
    "block-scoped-var": 2,
    "curly": 2,
    "default-case": 0,
    "dot-notation": 2,
    "dot-location": [
      2,
      "property"
    ],
    "eqeqeq": 2,
    "guard-for-in": 0,
    "no-alert": 1,
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-div-regex": 2,
    "no-else-return": 1,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-label": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 1,
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-spaces": 1,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": [
      2,
      "always"
    ],
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unmodified-loop-condition": 2,
    "no-unused-expressions": 2,
    "no-unused-labels": 2,
    "no-useless-call": 2,
    "no-useless-concat": 2,
    "no-useless-escape": 2,
    "no-void": 2,
    "no-warning-comments": 0,
    "no-with": 2,
    "radix": [
      1,
      "as-needed"
    ],
    "wrap-iife": [
      2,
      "outside"
    ],
    "yoda": [
      2,
      "never"
    ],
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-restricted-globals": [
      2,
      "event"
    ],
    "no-shadow-restricted-names": 2,
    "no-undef-init": 1,
    "no-undef": 2,
    "no-unused-vars": [
      2, {
        "vars": "all",
        "args": "none"
      }
    ],
    "no-use-before-define": 2,
    "handle-callback-err": 1,
    "no-mixed-requires": [
      2, {
        "grouping": true,
        "allowCall": true
      }
    ],
    "no-new-require": 2,
    "no-path-concat": 2,
    "no-restricted-imports": [
      2,
      "domain",
      "freelist",
      "smalloc",
      "sys",
      "colors"
    ],
    "no-restricted-modules": [
      2,
      "domain",
      "freelist",
      "smalloc",
      "sys",
      "colors"
    ],
    "array-bracket-spacing": 0,
    "brace-style": [
      2,
      "1tbs", {
        "allowSingleLine": false
      }
    ],
    "camelcase": [
      1, {
        "properties": "never"
      }
    ],
    "comma-spacing": [
      1, {
        "after": true,
        "before": false
      }
    ],
    "comma-style": [
      2,
      "last"
    ],
    "computed-property-spacing": [
      2,
      "never"
    ],
    "eol-last": [1, "never"],
    "indent": [
      1,
      4, {
        "SwitchCase": 1,
        "VariableDeclarator": 1
      }
    ],
    "jsx-quotes": 2,
    "key-spacing": [
      2, {
        "beforeColon": false,
        "afterColon": true
      }
    ],
    "keyword-spacing": [
      2, {
        "before": true,
        "after": true
      }
    ],
    "linebreak-style": [
      2,
      "unix"
    ],
    "max-nested-callbacks": [
      1,
      4
    ],
    "max-statements-per-line": 2,
    "new-cap": 0,
    "new-parens": 2,
    "no-array-constructor": 2,
    "no-lonely-if": 2,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": 2,
    "no-nested-ternary": 2,
    "no-negated-condition": 0,
    "no-new-object": 2,
    "no-restricted-syntax": [
      2,
      "WithStatement"
    ],
    "no-whitespace-before-property": 2,
    "no-spaced-func": 0,
    "no-trailing-spaces": 2,
    "no-unneeded-ternary": 2,
    "object-curly-spacing": 0,
    "one-var": 0,
    "one-var-declaration-per-line": 2,
    "operator-assignment": 1,
    "operator-linebreak": [
      2,
      "after"
    ],
    "padded-blocks": 0,
    "quote-props": 0,
    "quotes": 0,
    "semi-spacing": [
      2, {
        "before": false,
        "after": true
      }
    ],
    "semi": [
      2,
      "always"
    ],
    "space-before-blocks": [
      2,
      "always"
    ],
    "space-before-function-paren": [
      1,
      "always"
    ],
    "space-in-parens": 0,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": [
      1,
      "always"
    ],
    "arrow-parens": [
      2,
      "as-needed"
    ],
    "arrow-spacing": [
      2, {
        "before": true,
        "after": true
      }
    ],
    "constructor-super": 2,
    "generator-star-spacing": [
      2,
      "both"
    ],
    "no-class-assign": 2,
    "no-const-assign": 2,
    "no-dupe-class-members": 2,
    "no-duplicate-imports": [
      2, {
        "includeExports": true
      }
    ],
    "no-new-symbol": 2,
    "no-this-before-super": 2,
    "no-useless-computed-key": 0,
    "no-useless-constructor": 2,
    "require-yield": 2,
    "template-curly-spacing": 2,
    "yield-star-spacing": [
      2,
      "both"
    ],
    "no-shadow": 2,
    "strict": [
      2,
      "function"
    ],
    "max-len": [
      2, {
        "code": 120,
        "tabWidth": 4
      }
    ],
    "no-console": 1,
    "no-extra-parens": 0,
    "valid-jsdoc": 0,
    "no-empty-function": 0,
    "no-magic-numbers": 0,
    "no-undefined": 0,
    "require-jsdoc": 0,
    "wrap-regex": 1,
    "react/jsx-boolean-value": 2,
    "react/jsx-closing-bracket-location": [
      0,
      "props-aligned"
    ],
    "react/jsx-curly-spacing": [
      2,
      "never"
    ],
    "react/jsx-equals-spacing": [
      2,
      "never"
    ],
    "react/jsx-first-prop-new-line": [
      0,
      "multiline"
    ],
    "react/jsx-handler-names": 0,
    "react/jsx-indent-props": [
      1,
      4
    ],
    "react/jsx-indent": [
      1,
      4
    ],
    "react/jsx-key": 2,
    "react/jsx-no-bind": 2,
    "react/jsx-no-duplicate-props": [
      2, {
        "ignoreCase": true
      }
    ],
    "react/jsx-no-undef": 2,
    "react/jsx-pascal-case": 0,
    "react/jsx-space-before-closing": [
      2,
      "always"
    ],
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/no-danger": 2,
    "react/no-deprecated": 2,
    "react/no-did-update-set-state": 2,
    "react/no-direct-mutation-state": 2,
    "react/no-is-mounted": 2,
    "react/no-string-refs": 0,
    "react/no-unknown-property": 2,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 2,
    "react/require-extension": [
      0, {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/self-closing-comp": 2,
    "react/jsx-wrap-multilines": 1,
    "react/no-find-dom-node": 0,
    "react/display-name": 0,
    "zmclient/no-i18n": [0, {
      "checkCase": false
    }]
  }
}